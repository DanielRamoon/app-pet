{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"jsx": "react-native",
		"lib": [
			"dom",
			"esnext"
		],
		"strict": true,
		"strictFunctionTypes": false,
		"strictNullChecks": true,
		"strictPropertyInitialization": false,
		"esModuleInterop": true,
		"baseUrl": "./",
		"paths": {
			"@components/*": [
				"./src/components/*"
			],
			"@screens/*": [
				"./src/screens/*"
			],
			"@utils/*": [
				"./src/utils/*"
			],
			"@navigation/*": [
				"./src/navigation/*"
			],
			"@styles/*": [
				"./src/styles/*"
			],
			"@hooks/*": [
				"./src/hooks/*"
			],
			"@providers/*": [
				"./src/providers/*"
			],
			"@assets/*": [
				"./assets/*"
			],
			"types/*": [
				"./src/@types/*"
			],
			"@models/*": [
				"src/utils/supabase/services/*"
			],
		}
	}
}